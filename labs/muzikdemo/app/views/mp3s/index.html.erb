<h1>Listing mp3s</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Song</th>
    <th>Youtube</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @mp3s.each do |mp3| %>
  <tr>
    <td><%= mp3.name %></td>
    <td><%= mp3.song %></td>
    <td><%= mp3.youtube %></td>
    <td><%= link_to 'Show', mp3 %></td>
    <td><%= link_to 'Edit', edit_mp3_path(mp3) %></td>
    <td><%= link_to 'Destroy', mp3, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Mp3', new_mp3_path %>
